upstream {{getv "/subdomains/resume"}} {
{{range getvs "/services/resume/*"}}
    server {{.}};
{{end}}
}

server {
    server_name  {{getv "/subdomains/resume"}}.jarenglover.com;
    location / {
        proxy_pass        http://{{getv "/subdomains/resume"}};
        proxy_redirect    off;
        proxy_set_header  Host             $host;
        proxy_set_header  X-Real-IP        $remote_addr;
        proxy_set_header  X-Forwarded-For  $proxy_add_x_forwarded_for;
   }
}
